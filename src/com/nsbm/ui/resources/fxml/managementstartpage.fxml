<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" prefHeight="667.0" prefWidth="1005.0" spacing="15.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="titleText" text="Select a student to continue">
         <font>
            <Font name="Segoe UI" size="21.0" />
         </font>
      </Label>
      <TextField fx:id="studentID" maxWidth="-Infinity" prefHeight="39.0" prefWidth="387.75" promptText="Enter student ID here">
         <font>
            <Font size="14.25" />
         </font>
      </TextField>
      <Button fx:id="continueBtn" mnemonicParsing="false" text="Continue">
         <font>
            <Font name="Segoe UI" size="20.25" />
         </font>
      </Button>
   </children>
</fx:root>
